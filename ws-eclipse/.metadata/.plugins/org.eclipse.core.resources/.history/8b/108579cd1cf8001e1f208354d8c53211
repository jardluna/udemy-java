
public class Main {

	public static void main(String[] args) {
		
		int y = 32;
		double x = 10.357841;
		
		System.out.println("Olá Mundo!");
		System.out.println(y);
		System.out.println(x);
		System.out.printf("Número formatado com duas casa decimais %.2f %n", x);
		System.out.printf("%.4f %n", x);
		
	}
}
